{
  "name": "bocf",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/bocf-cli.js",
  "module": "src/bocf-cli.js",
  "description": "build OCI Image Format images",
  "keywords": [
    "container",
    "OCI"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "bocf": "bin/bocf"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/bocf-cli.js --section=API",
    "lint": "documentation lint src/bocf-cli.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "caporal": "^0.10.0",
    "config-expander": "^10.2.5",
    "pump": "^3.0.0",
    "tar-stream": "^1.5.5",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "builtin-modules": "^3.1.0",
    "c8": "^5.0.2",
    "documentation": "^11.0.1",
    "esm": "^3.2.25",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.16.4",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-executable": "^1.5.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.18"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/bocf.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/bocf/issues"
  },
  "homepage": "https://github.com/arlac77/bocf#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "snyk": true
}
